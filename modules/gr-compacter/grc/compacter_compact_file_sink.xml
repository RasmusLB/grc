<block>
  <name>Compact file sink</name>
  <key>compacter_compact_file_sink</key>
  <category>[COMPACTER]</category>
  <import>import compacter</import>
  <make>compacter.compact_file_sink($vlen,$*filename, $append)</make>
  <param>
    <name>filename</name>
    <key>filename</key>
    <type>string</type>
  </param>
  <param>
    <name>Append</name>
    <key>append</key>
    <value>False</value>
    <type>bool</type>
  </param>
  <param>
    <name>Sample rate</name>
    <key>sample_rate</key>
    <type>int</type>
  </param>
  <param>
    <name>fft size</name>
    <key>fft_size</key>
    <type>int</type>
  </param>
  <param>
    <name>Center frequency</name>
    <key>center_freq</key>
    <type>int</type>
  </param>
  <param>
    <name>Threshold</name>
    <key>threshold</key>
    <type>float</type>
  </param>
  <param>
    <name>vlen</name>
    <key>vlen</key>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>${ vlen }</vlen>
  </sink>
</block>
